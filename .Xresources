! transparency - true or false (default)
!URxvt*transparent: true

! tint with any color; i.e., blue, red, tomato4, olivedrab2, etc.
!   some nice listings are at:
!     http://www.nisrv.com/drupal/?q=node/11
!     http://www.htmlgoodies.com/tutorials/colors/article.php/3478921
!URxvt*tintColor: S_base03
!URxvt*shading: 15



!Xft.hintstyle:  hintfull
Xft.antialias:          1
Xft.hinting:            1
Xft.rgba:               rgb
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault
URxvt*scrollBar: off
URxvt*reverseVideo: on
!URxvt*font: xft:Code2000:antialias=false
!URxvt*font: xft:bitstream vera sans mono:size=12:antialias=true
!URxvt*font: xft:Monospace:pixelsize=15
! **this works in kali
! apt-get install fonts-unifont xfonts-unifont ttf-unifont
URxvt*font: xft: Unifont:antialias=true,xft: Unifont Upper:pixelsize=15:antialias=true:pixelsize=15
!fallback to xterm, antialias=false means no utf
xterm*faceName: Unifont:size=14:antialias=false
xterm*faceSize: 14
xterm*boldFaceName: Unifont:size=14
xterm*renderFont: false
XTerm*font: 8x15
XTerm*utf8: 1
!URxvt*font: 7x14
URxvt*urgentOnBell: true
